{
    "openapi": "3.0.0",
    "info": {
      "title": "List API",
      "description": "API Description",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:4100"
      }
    ],
    "paths": {
      "/": {
        "get": {
          "summary": "Mengambil data dari server.",
          "description": "Endpoint untuk mengambil data dari server.",
          "responses": {
            "200": {
              "description": "Data berhasil diambil."
            },
            "500": {
              "description": "Terjadi kesalahan pada server."
            }
          }
        }
      },
      "/addData": {
        "post": {
          "summary": "Menambahkan data karyawan dengan insentif ke server.",
          "description": "Endpoint untuk menambahkan data karyawan dengan insentif ke server.",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "karyawan": {
                      "type": "string"
                    },
                    "insentif": {
                      "type": "string"
                    },
                    "jumlah": {
                      "type": "number"
                    }
                  },
                  "required": ["karyawan", "insentif", "jumlah"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Data berhasil ditambahkan."
            },
            "400": {
              "description": "Data tidak boleh kosong."
            },
            "500": {
              "description": "Terjadi kesalahan pada server."
            }
          }
        }
      },
      "/editContact/{id}": {
        "put": {
          "summary": "Mengubah data karyawan.",
          "description": "Endpoint untuk mengubah data karyawan.",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID data yang akan diubah.",
              "required": true,
              "schema": {
                "type": "integer"
              }
            }
          ],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "karyawan": {
                      "type": "string"
                    },
                    "insentif": {
                      "type": "string"
                    },
                    "jumlah": {
                      "type": "number"
                    }
                  },
                  "required": ["karyawan", "insentif", "jumlah"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Data berhasil diubah."
            },
            "400": {
              "description": "Data tidak boleh kosong."
            },
            "500": {
              "description": "Terjadi kesalahan pada server."
            }
          }
        }
      },
      "/deleteData/{id}": {
        "delete": {
            "summary": "Menghapus data karyawan dari server.",
            "description": "Endpoint untuk menghapus data karyawan.",
            "parameters": [
                {
                    "name": "id",
                    "in": "path",
                    "description": "ID data yang akan dihapus.",
                    "required": true,
                    "schema": {
                        "type": "integer"
                    }
                }
            ],
            "responses": {
                "200": {
                    "description":"Data berhasil dihapus."
                },
                "500": {
                    "description":"Terjadi kesalahan pada server."
                }
            }
        }
      },
      "/search": {
        "get": {
            "summary":"Mencari data dari server.",
            "description": "Endpoint untuk mencari data karyawan.",
            "parameters": [
                {
                    "name": "karyawan",
                    "in": "quary",
                    "description":"Data berhasil ditemukan.",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ],
            "responses": {
                "200": {
                    "description": "Data berhasil ditemukan."
                },
                "500": {
                    "description": "Terjadi kesalahan saat melakukan pencarian"
                }
            }
        }
      },
      "/login": {
        "post": {
            "summary": "Login User",
            "description":"Endpoint Login.",
            "requestBody": {
                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties":{
                                "usernameOrEmail": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                }
                            },
                            "required": ["usernameOrEmail", "password"]
                        }
                    }
                }
            },
            "responses": {
                "200":{
                    "description": "Login berhasil"
                },
                "500": {
                    "description": "Error Login"
                },
                "401": {
                    "description": "Email Atau Password tidak ditemukan"
                }
            }
        }
      },
      "/logout": {
        "post": {
            "summary": "Logout",
            "description": "Endpoint untuk Logout user",
            "responses": {
                "200": {
                    "description": "Logout berhasil"
                },
                "500": {
                    "description": "Error logout"
                }
            }
        }
      }
    }
  }